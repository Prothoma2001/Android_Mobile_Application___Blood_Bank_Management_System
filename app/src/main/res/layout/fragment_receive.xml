<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/receive"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.receive.ReceiveFragment">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="459dp"
        android:layout_height="839dp"
        app:srcCompat="@drawable/img_2" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/navigationView"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="#181A1C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="153dp"
            android:layout_height="77dp"
            android:layout_marginEnd="230dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logoname" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/bloodbank"
        android:layout_width="253dp"
        android:layout_height="50dp"
        android:text="Blood Bank"
        android:textColor="#181A1C"
        android:textStyle="bold"
        app:backgroundTint="#ECEFF3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.468"
        app:strokeColor="#ECEFF3"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/donorlist"
        android:layout_width="253dp"
        android:layout_height="50dp"
        android:text="Donor LIst"
        android:textColor="#181A1C"
        android:textStyle="bold"
        app:backgroundTint="#ECEFF3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.594"
        app:strokeColor="#ECEFF3"
        app:strokeWidth="1dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="172dp"
        android:layout_height="305dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037"
        app:srcCompat="@drawable/receive2" />


</androidx.constraintlayout.widget.ConstraintLayout>